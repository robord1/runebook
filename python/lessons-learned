












LESSON 1


	func isOperator(token: String) -> Bool{
	    ["+", "-", "*", "/"].contains(token)
	}


	func isOperator(token: String) -> Bool{
	    
	    if token == "+" || token == "-" || token == "*" || token == "/"{
	        return true
	    }else{
	        return false
	    }
	}


